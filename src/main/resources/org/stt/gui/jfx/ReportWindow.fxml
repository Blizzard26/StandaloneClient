<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="borderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <Button mnemonicParsing="false" onAction="#closeWindow" text="%report.done" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
   <top>
      <FlowPane fx:id="reportControlsPane" hgap="5.0" vgap="5.0" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </FlowPane>
   </top>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="tableForReport" prefHeight="300.0" prefWidth="400.0">
               <columns>
                  <TableColumn fx:id="columnForDuration" prefWidth="89.0" text="%report.duration" />
                  <TableColumn fx:id="columnForComment" prefWidth="319.0" text="%report.comment" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <HBox prefHeight="100.0" spacing="10.0">
               <children>
                  <Label text="%startOfReport" />
                  <Label fx:id="startOfReport" text="Label" />
                  <Label text="%endOfReport" />
                  <Label fx:id="endOfReport" text="Label" />
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
